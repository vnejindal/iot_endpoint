{
   "_comment":"Template file for onboard gyroscope on Udoo Neo Full",
   "sensor":
        {
            "type":"gyroscope",
            "interface":"i2c",
            "fs_path":"/sensors/gyroscope",
            "files":[
                {
                  "control_file":"enable"
                },
                {
                  "data_file":"data"
                }
            ]
         },
    "default_unit":"tbd",
    "unit":"tbd",
    "state":"disabled",
    "status":"none",
    "sleep":10,
    "trigger":
    	{
    		"state":"off",
    		"high_value":"tbd",
    		"low_value":"tbd",
    		"unit":"tbd",
    		"trigger_action":"alarm"
        }
}