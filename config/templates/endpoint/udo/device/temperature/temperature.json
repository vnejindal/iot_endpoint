{
   "_comment":"Template file for i2c temperature sensor on Udoo Neo Full",
   "sensor":
        {
            "type":"Temperature",
            "interface":"i2c",
            "fs_path":"/sensors/barometer",
            "files":[
                {
                  "data_file":"in_temp_scale"
                },
                {
                  "data_file":"in_temp_raw"
                }
            ]
         },
    "default_unit":"celcius",
    "unit":"celcius",
    "state":"disabled",
    "status":"none",
    "sleep":10,
    "trigger":
    	{
    		"state":"off",
    		"high_value":78,
    		"low_value":23,
    		"unit":"celcius",
    		"trigger_action":"alarm"
        }
}